{
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jquery": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
        "semi": 0,
        "strict": 0,
        "no-console": 0,
        "comma-dangle": ["error", "always"]
    }
}